<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tsofen.onthegoshopClient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".NewDriver"></activity>
        <activity android:name=".newProduct" />
        <activity android:name=".ManagerOrders" />
        <activity android:name=".ManagerUsers" />
        <activity android:name=".ManagerVans" />
        <activity android:name=".ManagerDrivers" />
        <activity android:name=".ManagerStorage" />
        <activity android:name=".ManagerMain" />
        <activity
            android:name=".UserMainView"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>